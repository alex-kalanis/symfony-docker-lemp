{
    "name": "alex-kalanis/kw_paths",
    "description": "Paths for accessing targets in KWCMS",
    "keywords": [
        "filesystem",
        "files",
        "path",
        "paths"
    ],
    "license": "BSD-3-Clause",
    "type": "library",
    "require": {
        "ext-mbstring": "*",
        "php": ">=7.2.0"
    },
    "require-dev": {
        "alex-kalanis/kw_input": ">=2.2",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": ">=7.2"
    },
    "suggest": {
        "alex-kalanis/kw_input": "Inputs put in the same way - basic interfaces"
    },
    "autoload": {
        "psr-4": {
            "kalanis\\kw_paths\\": "php-src/"
        }
    },
    "scripts": {
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
